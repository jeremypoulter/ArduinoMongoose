{
  "name": "ArduinoMongoose",
  "keywords": "network,http,mqtt,tcp,udp,ssl,tls,sntp",
  "description": "Arduino style wrapper around Mongoose library. HTTP client/server, MQTT client",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/jeremypoulter/ArduinoMongoose.git"
  },
  "authors":
  [
    {
      "name": "Jeremy Poulter",
      "email": "jeremy@bigjungle.net",
      "maintainer": true
    }
  ],
  "examples": [
    {
      "name": "esp_ota_http_server",
      "base": "examples/esp_ota_http_server",
      "files": [
        "src/esp_ota_http_server.cpp"
      ]
    },
    {
      "name": "simple_http_client",
      "base": "examples/simple_http_client",
      "files": [
        "src/simple_http_client.cpp"
      ]
    },
    {
      "name": "streaming_http_client_native",
      "base": "examples/streaming_http_client_native",
      "files": [
        "src/simple_http_client.cpp"
      ]
    },
    {
      "name": "simple_http_server",
      "base": "examples/simple_http_server",
      "files": [
        "src/simple_http_server.cpp"
      ]
    },
    {
      "name": "simple_mqtt_client",
      "base": "examples/simple_mqtt_client",
      "files": [
        "src/simple_mqtt_client.cpp"
      ]
    },
    {
      "name": "simplest_web_server_esp",
      "base": "examples/simplest_web_server_esp",
      "files": [
        "src/simplest_web_server.cpp"
      ]
    },
    {
      "name": "simplest_web_server_native",
      "base": "examples/simplest_web_server_native",
      "files": [
        "src/simplest_web_server.c"
      ]
    },
    {
      "name": "sntp_client",
      "base": "examples/sntp_client",
      "files": [
        "src/sntp_client.cpp"
      ]
    },
    {
      "name": "websocket_chat",
      "base": "examples/websocket_chat",
      "files": [
        "src/websocket_chat.cpp"
